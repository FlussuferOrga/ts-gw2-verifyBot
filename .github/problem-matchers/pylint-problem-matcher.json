{
    "problemMatcher": [
        {
            "owner": "pylint",
            "pattern": [
                {
                    "regexp": "^(?>(error|warning|info):)?(.+):(\\d+):(\\d+):\\s(.+):\\s(.+)$",
                    "severity" : 1,
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "code": 5,
                    "message": 6
                }
            ]
        }
    ]
}